<body id="fmain">
    <div class="container" id="ash">
    <form action="/myform" method="post">
        <input type="text" name="q">
        <button>Search google</button>
        <p>
            <label for="your name">Student name</label>
            <br>
            <input type="text" placeholder="you name" name="myname">
            
        </p>
        
        <p>
            <label for="date">Enter DOB:</label>
            <br>
            <input type="date" placeholder="DOB" name="DOB">
        </p>
        <p>
            <label for="pass">Enter Year of passing:</label>
            <br>
            <input type="year" placeholder="ypass" name="ypass">
        </p>
        
            <p>Enter your branch</p>
            <br>
            <div>
                <input type="radio" name="branch" value="cs" name="branch">
                <label for="CS">CS</label>
                
            </div>
            <div>
                <input type="radio" name="branch" value="it" name="branch">
                <label for="IT">IT</label>
                
            </div>
            <div>
                <input type="radio" name="branch" value="extc" name="branch">
                <label for="EXTC">EXTC</label>
                
            </div>
            <div>
                <input type="radio" name="branch" value="MECH" name="branch">
                <label for="mech">MECH</label>
                
            </div>
            
            
        
        <p>
            <label for="Company">Select you company</label>
            <br>
            <select name="Company" id="Company" name="company">
                <option value="none">none</option>
                <option value="tcs">tcs</option>
                <option value="ibm">IBM</option>
                <option value="Amazon">Amazon</option>
                <option value="google">Google</option>
                <option value="microsoft">microsoft</option>

            </select>
        </p>
        <p>
            <label for="your skill">Write your Skill</label>
            <br>
            <textarea id="skill" rows="5" cols="40" placeholder="EnteR YOUR SKILL HERE" name="skill"></textarea>
        </p>
        <input type="submit" class="btn btn-primary" value="submit">
        <button type="button" id="b1" >Submit</button>
        

    </form>
    
</div>
